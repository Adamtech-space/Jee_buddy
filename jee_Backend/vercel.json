{
    "version": 2,
    "name": "jee-buddy-backend",
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["**/*"],
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ],
    "env": {
        "NODE_ENV": "development"
    },
    "github": {
        "enabled": true,
        "silent": true
    },
    "functions": {
        "index.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
}
