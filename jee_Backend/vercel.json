{
    "version": 2,
    "name": "jee-buddy-backend",
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["**/*"],
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production",
        "NPM_FLAGS": "--legacy-peer-deps"
    },
    "github": {
        "enabled": true,
        "silent": true
    }
}
