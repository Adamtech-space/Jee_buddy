{
    "builds": [
        {
            "src": "src/core/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/core/wsgi.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "core.settings",
        "PYTHONPATH": "src"
    }
}
