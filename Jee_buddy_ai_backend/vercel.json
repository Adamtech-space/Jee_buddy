{
    "version": 2,
    "builds": [
        {
            "src": "src/core/vercel_wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.12.7"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/core/vercel_wsgi.py"
        }
    ],
    "env": {
        "PYTHONPATH": "src"
    }
}
