{
    "version": 2,
    "functions": {
        "src/core/wsgi.py": {
            "runtime": "python3.9",
            "maxDuration": 200,
            "memory": 512
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/core/wsgi.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "core.settings",
        "PYTHONPATH": "src",
        "DJANGO_ALLOWED_HOSTS": ".vercel.app",
        "DJANGO_CONFIGURATION": "Production"
    }
}
